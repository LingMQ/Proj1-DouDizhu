<body background="https://i.ibb.co/hfFqxT5/di2.png">
<div class="row">
  <div class="column">
    <h1 align= "left">Fight Against Landlord </h1>
    <div class="row" align="left">
      <%= form_for @conn, "/game", fn f -> %>
        			<p>Enter your user name: <%= text_input f, :player, placeholder: "awesome_me" %></p>
        			<p>Enter a room name: <%= text_input f, :name, placeholder: "demo" %></p>
        			<p><%= submit "Join Game!"%></p>
        		<% end %>
    </div>
  </div>
</div>
